{
  "name": "html-tokenizer",
  "version": "1.0.0",
  "description": "html tokenizer",
  "main": "dist/index.js",
  "keywords": [
    "html",
    "tokenizer",
    "lexer",
    "scanner",
    "token"
  ],
  "author": "espretto",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "typescript": "^3.9.6",
    "jest": "^26.1.0",
    "@types/jest": "^26.0.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": ["babel-jest"],
      "\\.test$": ["./jsonTransformer.js"]
    }
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version"
    ]
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "tsc --build ./tsconfig.json",
    "lint:types": "tsc --noEmit ./src/**/*"
  }
}